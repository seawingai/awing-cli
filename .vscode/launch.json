    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach",
            "port": 9229,
            "request": "attach",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
        {
            "type": "node-terminal",
            "name": "npm run dev",
            "request": "launch",
            "command": "npm run dev",
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "node-terminal",
            "name": "db migrate",
            "request": "launch",
            "command": "npx tsx src/index.ts db migrate --env=dev",
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "node-terminal",
            "name": "db seed-up",
            "request": "launch",
            "command": "npx tsx src/index.ts db seed up",
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "node-terminal",
            "name": "g saas",
            "request": "launch",
            "command": "npx tsx src/index.ts g saas xyz",
            "cwd": "${workspaceFolder}"
        }
    ]
}