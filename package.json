{
  "name": "awing-cli",
  "version": "0.0.1",
  "type": "module",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "nx build",
    "test": "nx test",
    "start": "node dist/cli.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "help": "npx tsx src/index.ts",
    "con:l": "npx tsx src/index.ts cfg list",
    "db:m": "npx tsx src/index.ts db migrate --env=dev",
    "db:mp": "npx tsx src/index.ts db migrate --env=prod",
    "db:seed": "npx tsx src/index.ts db schema",
    "db:su": "npx tsx src/index.ts db seed up",
    "g:saas": "npx tsx src/index.ts g saas xyz",
    "g:s": "npx tsx src/index.ts g service settings-service",
    "g:l": "npx tsx src/index.ts g lib awing-core"
  },
  "nx": {
    "includedScripts": []
  },
  "dependencies": {
    "commander": "^14.0.0"
  },
  "bin": {
    "aw": "dist/index.js"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@nx/esbuild": "^21.2.1",
    "@nx/eslint": "21.2.1",
    "@nx/eslint-plugin": "21.2.1",
    "@nx/js": "21.2.1",
    "@nx/vite": "21.2.1",
    "@nx/web": "21.2.1",
    "@nx/workspace": "21.2.1",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/node": "18.16.9",
    "@vitest/coverage-v8": "^3.0.5",
    "@vitest/ui": "^3.0.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "jiti": "2.4.2",
    "jsonc-eslint-parser": "^2.4.0",
    "nx": "21.2.1",
    "prettier": "^2.6.2",
    "tslib": "^2.3.0",
    "tsx": "^4.20.3",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.29.0",
    "vite": "^6.0.0",
    "vitest": "^3.0.0"
  }
}
